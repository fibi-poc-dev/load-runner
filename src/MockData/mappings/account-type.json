{
  "request": {
    "method": "GET",
    "urlPathPattern": "/bfb-ils-transactions-booked/api/v1/accountType/.*",
    "headers": {
      "Authorization": {
        "matches": "Bearer .*"
      }
    }
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "accountType": "{{randomValue values=['CHECKING', 'SAVINGS', 'BUSINESS']}}",
      "accountId": "{{request.pathSegments.[6]}}",
      "branchId": "{{request.pathSegments.[5]}}",
      "accountStatus": "ACTIVE",
      "currency": "ILS",
      "balance": "{{randomDecimal min=0.0 max=50000.0}}"
    },
    "transformers": ["response-template"]
  }
}
